---
import { Image } from 'astro:assets';
import avatar from "../assets/avatar.png"
const { text } = Astro.props;
const { URL } = Astro.props;
---
<a href={ URL }>
    <button class="transition duration-300 w-40 bg-pink-500 hover:bg-pink-700 text-white font-bold py-2 px-4 rounded-3xl">
        {text}
    </button>
</a>
